{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Cache": {
    "MaxEntities": 5,
    "MaxQueries": 7,
    "Overrides": [
      {
        "Type": "ExampleEntity",
        "MaxEntities": 10
      }
    ],
    "Preload": [ "ExampleEntity" ],
    "Exclude": [ "LoginEntity" ]
  },
  "AllowedHosts": "*",
  "Assemblies": [
    "Core.Server.Application",
    "Core.Server.Common",
    "Core.Server.Persistence",
    "Core.Server.Shared",
    "Core.Server.Web"
  ]
}