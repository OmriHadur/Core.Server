{
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Application": "MethodsTimeInputOutput",
    "Repository": "MethodsTime"
  },
  "AllowAnonymous": [
    {
      "ResourceType": "Core.Server.Shared.Resources.ExampleResource,Core.Server.Shared",
      "ResourceActions": 15
    }
  ],
  "Cache": {
    "MaxEntities": 5,
    "MaxQueries": 7,
    "Overrides": [
      {
        "Type": "Example",
        "MaxEntities": 10
      }
    ],
    "Preload": [ "Example" ],
    "Exclude": [ "Login", "User" ]
  },
  "AllowedHosts": "*",
  "Assemblies": [
    "Core.Server.Application",
    "Core.Server.Common",
    "Core.Server.Persistence",
    "Core.Server.Shared",
    "Core.Server.Web"
  ]
}