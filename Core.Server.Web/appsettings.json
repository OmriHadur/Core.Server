{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Application": "Started, Took, Request, Response",
    "Repository": "Started, Took"
  },
  "Cache": {
    "MaxEntities": 5,
    "MaxQueries": 7,
    "Overrides": [
      {
        "Type": "Example",
        "MaxEntities": 10
      }
    ],
    "Preload": [ "Example" ],
    "Exclude": [ "Login", "User" ]
  },
  "AllowedHosts": "*",
  "Assemblies": [
    "Core.Server.Application",
    "Core.Server.Common",
    "Core.Server.Persistence",
    "Core.Server.Shared",
    "Core.Server.Web"
  ]
}